		</category>
		<category dictKey="tv_detail" dictValue="tv_detail">
			<template>html/tv_detailpage.xml</template>
		</category>
		<category dictKey="website_index" dictValue="website_index">
			<template>website/indexpage.xml</template>
		</category>
		<category dictKey="website_detail" dictValue="website_detail">
			<template>website/detailpage.xml</template>
		</category>
		<category dictKey="lcApp" dictValue="lcApp(增删改)">
			<template>lcApp/controller.xml</template>
			<template>lcApp/dao.xml</template>
			<template>lcApp/entity.xml</template>
			<template>lcApp/mapper.xml</template>
