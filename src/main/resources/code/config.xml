<?xml version="1.0" encoding="utf-8"?>
<config>
	<!-- 生成分类 -->
	<category>

		<category dictKey="springcloud" dictValue="springcloud-2.0-接口版">
			<template>springcloud/controller.xml</template>
			<template>common/dao.xml</template>
			<template>common/entity.xml</template>
			<template>common/mapper.xml</template>
			<template>common/service.xml</template>
			<template>common/iservice.xml</template>
			<template>common/helperservice.xml</template>
		</category>

		<category dictKey="springcloud_open" dictValue="springcloud-open-接口版">
			<template>springcloud_open/controller.xml</template>
			<template>common/dao.xml</template>
			<template>common/entity.xml</template>
			<template>common/mapper.xml</template>
			<template>common/service.xml</template>
			<template>common/iservice.xml</template>
			<template>common/helperservice.xml</template>
		</category>


		<category dictKey="job_admin" dictValue="job-admin_自给数据">
			<template>job_admin/controller.xml</template>
			<template>job_admin/service.xml</template>
			<template>common/helperservice.xml</template>
			<template>common/dao.xml</template>
			<template>common/mapper.xml</template>
